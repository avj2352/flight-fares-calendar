{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@types/react-dom": "^16.9.0",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-notifications-component": "^2.0.7",
    "react-scripts": "3.1.1",
    "typescript": "^3.6.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "dev": "npm run watch:css & react-scripts start",
    "build:css": "postcss src/assets/css/main.css -o src/index.css",
    "watch:css": "postcss src/assets/css/main.css -o src/index.css -w",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tailwindcss/custom-forms": "^0.2.1",
    "autoprefixer": "^9.6.1",
    "postcss-cli": "^6.1.3",
    "tailwindcss": "^1.1.2"
  }
}
